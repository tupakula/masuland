<?xml version="1.0" encoding="utf-8"?>
<objects
	xmlns="http://www.springactionscript.org/schema/objects"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springactionscript.org/schema/objects
		http://www.springactionscript.org/schema/objects/spring-actionscript-objects-1.0.xsd"
	>
	
	<!-- PROCESSOR -->	
	
	<object 
		id="autowiringStageProcessor" 
		class="org.springextensions.actionscript.stage.DefaultAutowiringStageProcessor"
		/>
		
	<object 
		id="eventhandlerProcessor" 
		class="org.springextensions.actionscript.ioc.factory.config.EventHandlerMetadataProcessor"
		/>
		
	<object 
		id="routeEventsProcessor" 
		class="org.springextensions.actionscript.ioc.factory.config.RouteEventsMetaDataProcessor"
		/>
	
	<!-- SERVICE -->	
<!--	
	<object 
		id="appRemoteObject" 
		class="mx.rpc.remoting.mxml.RemoteObject"
		/>
-->	
	<!-- DELEGATE -->	
<!--	
	<object 
		id="appDelegate" 
		class="com.masuland.loginexample.business.AppAmfDelegate"
		/>
-->	
	<object 
		id="appDelegate" 
		class="com.masuland.loginexample.business.AppMockDelegate"
		/>
	
	<!-- COMMAND -->	
	
	<object class="com.masuland.loginexample.control.command.InitAppCommand" />
	<object class="com.masuland.loginexample.control.command.GetSettingsCommand" />
	<object class="com.masuland.loginexample.control.command.LoadLayoutCommand" />
	<object class="com.masuland.loginexample.control.command.LoadStyleCommand" />
	<object class="com.masuland.loginexample.control.command.LoadLocaleCommand" />
	
	<object class="com.masuland.loginexample.control.command.GotoLoginCommand" />
	<object class="com.masuland.loginexample.control.command.GotoRegisterCommand" />
	<object class="com.masuland.loginexample.control.command.LoginCommand" />
	<object class="com.masuland.loginexample.control.command.RegisterCommand" />
	<object class="com.masuland.loginexample.control.command.UpdateUserCommand" />
	<object class="com.masuland.loginexample.control.command.LogoutCommand" />
	
	<!-- MODEL -->	
	
	<object 
		id="appModel" 
		class="com.masuland.loginexample.model.AppModel"
		/>

	<!-- CODE BEHIND -->
	
	<object 
		id="settingsBoxCB" 
		class="com.masuland.loginexample.view.cb.SettingsBoxCB"
		/>

	<object 
		id="loginBoxCB" 
		class="com.masuland.loginexample.view.cb.LoginBoxCB"
		/>

	<object 
		id="userBoxCB" 
		class="com.masuland.loginexample.view.cb.UserBoxCB"
		/>

</objects>





